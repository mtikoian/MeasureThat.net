@using Humanizer
@using System.Threading.Tasks
@using MeasureThat.Net.Logic.Web
@model IEnumerable<MeasureThat.Net.Models.BenchmarkDto>
@{
    Layout = null;
}

<ul>
    @foreach (var b in Model)
            {
        <li>
            <h4><a asp-action="Show" asp-route-id="@b.Id" asp-route-version="@b.Version" asp-route-name="@SeoFriendlyStringConverter.Convert(b.BenchmarkName)">@b.BenchmarkName</a>(<small>@b.WhenCreated.Humanize()</small>)</h4>
        </li>
    }
</ul>
